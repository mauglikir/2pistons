<html dir="{{ direction }}" lang="{{ lang }}">
<!--<![endif]-->
<head>
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<title>{{ title }}</title>
<base href="{{ base }}" />
{% if description %}
<meta name="description" content="{{ description }}" />
{% endif %}
{% if keywords %}
<meta name="keywords" content="{{ keywords }}" />
{% endif %}
<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
<link rel="manifest" href="/site.webmanifest">
<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
<meta name="msapplication-TileColor" content="#da532c">
<meta name="theme-color" content="#ffffff">
<!-- Font Awesome -->
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css">
<script src="catalog/view/javascript/js/jquery-3.4.1.min.js"></script>
<link rel="stylesheet" href="catalog/view/javascript/css/bootstrap.css">
<link rel="stylesheet" href="catalog/view/javascript/css/mdb.css">
<link href="//fonts.googleapis.com/css?family=Open+Sans:400,400i,300,700" rel="stylesheet" type="text/css" />
<link href="catalog/view/theme/default/stylesheet/stylesheet.css" rel="stylesheet">
{% for style in styles %}
<link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
{% endfor %}
{% for script in scripts %}
<script src="{{ script }}" type="text/javascript"></script>
{% endfor %}
<script src="/catalog/view/javascript/js/popper.min.js"></script>
<script src="catalog/view/javascript/common.js" type="text/javascript"></script>
<script src="catalog/view/javascript/js/bootstrap.js" type="text/javascript"></script>
<script src="catalog/view/javascript/js/mdb.js" type="text/javascript"></script>
{% for link in links %}
<link href="{{ link.href }}" rel="{{ link.rel }}" />
{% endfor %}
{% for analytic in analytics %}
{{ analytic }}
{% endfor %}
</head>
<body>
<nav id="top">
  <div class="container">{{ currency }}
    {{ language }}
    <div id="top-links" class="nav pull-right">
      
      
      <ul class="list-inline nav">
        
        <li><a class="btn btn-top-nav"><i class="fa fa-phone"></i><span class="hidden-xs hidden-sm hidden-md">{{ telephone }}</span></a></li>
        {# <li class="dropdown nav-item"><a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> <span class="hidden-xs hidden-sm hidden-md">{{ text_account }}</span> <span class="caret"></span></a>
          <ul class="dropdown-menu dropdown-menu-right">
            {% if logged %}
            <li><a href="{{ account }}">{{ text_account }}</a></li>
            <li><a href="{{ order }}">{{ text_order }}</a></li>
            <li><a href="{{ transaction }}">{{ text_transaction }}</a></li>
            <li><a href="{{ download }}">{{ text_download }}</a></li>
            <li><a href="{{ logout }}">{{ text_logout }}</a></li>
            {% else %}
            <li><a href="{{ register }}">{{ text_register }}</a></li>
            <li><a href="{{ login }}">{{ text_login }}</a></li>
            {% endif %}
          </ul>
        </li> #}
        
        <li class="nav-item"><a class="btn btn-top-nav" href="{{ wishlist }}" id="wishlist-total" title="{{ text_wishlist }}"><i class="fa fa-heart"></i> <span class="hidden-xs hidden-sm hidden-md">{{ text_wishlist }}</span></a></li>
        {# <li class="nav-item"><a href="{{ shopping_cart }}" title="{{ text_shopping_cart }}"><i class="fa fa-shopping-cart"></i> <span class="hidden-xs hidden-sm hidden-md">{{ text_shopping_cart }}</span></a></li> #}
        {# <li class="nav-item"><a href="{{ checkout }}" title="{{ text_checkout }}"><i class="fa fa-share"></i> <span class="hidden-xs hidden-sm hidden-md">{{ text_checkout }}</span></a></li> #}
        <li class="nav-item"><a class="btn btn-top-nav" href="{{ contact }}">КОНТАКТЫ</a></li>
        <li class="nav-item"><a  class="btn btn-top-nav" href="{{ register }}">{{ text_register }}</a></li>
        <li class="nav-item"><a class="btn btn-red btn-top-nav" href="{{ login }}">{{ text_login }}</a></li>
      </ul>
    </div>
  </div>
</nav>
<header>
  <div class="container">
    <div class="row container-header">



      <!-- Navbar -->
<nav class="navbar navbar-expand-lg">

  <!-- Navbar brand -->
  <a class="navbar-brand text-uppercase" href="#">
    <div id="logo">{% if logo %}<a href="{{ home }}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive" /></a>{% else %}
      <h1><a href="{{ home }}">{{ name }}</a></h1>
      {% endif %}</div>
  </a>

  <!-- Collapse button -->
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent2"
    aria-controls="navbarSupportedContent2" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <!-- Collapsible content -->
  <div class="collapse navbar-collapse" id="navbarSupportedContent2">

    <!-- Links -->
    <ul class="navbar-nav mr-auto">
      {% for category in categories %}
        {% if category.children %}

      <!-- Features -->
      <li class="nav-item dropdown mega-dropdown dropdown-hover">
        <a href="{{ category.href }}" class="dropdown-toggle dropdown-toggle text-uppercase" id="navbarDropdownMenuLink2" data-toggle="dropdown"
        aria-haspopup="true" aria-expanded="false">{{ category.name }}</a>
        <div class="dropdown-menu mega-menu v-2 z-depth-1 py-5 px-3"
          aria-labelledby="navbarDropdownMenuLink2">
          <div class="dropdown-inner"> {% for children in category.children|batch(category.children|length / category.column|round(1, 'ceil')) %}
            <ul class="list-unstyled">
              {% for child in children %}
              <li><a href="{{ child.href }}">{{ child.name }}</a></li>
              {% endfor %}
            </ul>
            {% endfor %}</div>
          <a href="{{ category.href }}" class="see-all">{{ text_all }} {{ category.name }}</a> </div>
            
        
      </li>
    
      
      

      {% endif %}
        {% endfor %}

    </ul>
    
    <!-- Search form -->
    <form class="form-inline">
      <div class="md-form my-0">
        {{ search }}
      </div>
    </form>

    <form class="form-inline">
      <div class="md-form my-0">
        {{ cart }}
      </div>
    </form>

  </div>
  <!-- Collapsible content -->

</nav>
<!-- Navbar -->







      {# <div class="col-sm-2">
        <div id="logo">{% if logo %}<a href="{{ home }}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive" /></a>{% else %}
